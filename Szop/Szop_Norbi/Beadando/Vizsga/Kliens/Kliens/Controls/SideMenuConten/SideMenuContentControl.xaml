<UserControl x:Class="Kliens.SideMenuContentControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Kliens"
             xmlns:core="clr-namespace:Kliens.Core;assembly=Kliens.Core"  
             mc:Ignorable="d" 
             d:DesignHeight="430" d:DesignWidth="110">
    <Grid Background="{StaticResource BackgroundVeryLightBrush}">
      
        <Grid.RowDefinitions>
            <RowDefinition Height="70"/>
            <RowDefinition Height="70"/>
            <RowDefinition Height="70"/>
            <RowDefinition Height="70"/>
            <RowDefinition Height="70"/>
            <RowDefinition Height="70"/>
        </Grid.RowDefinitions>

        <Button Grid.Row="0" Content="Home" Click="Home_click"/>
        <Button Grid.Row="1" Content="Add" Click="Add_Click"/>
        <Button Grid.Row="2" Content="Upd" Click="Upd_Click"/>
        <Button Grid.Row="3" Content="Del" Click="Del_Click"/>
        <Button Grid.Row="4" Content="Fuel" Click="Fuel_Click"/>
        <Button Grid.Row="5" Content="Docs" Click="Doc_Click"/>


    </Grid>
</UserControl>